{
    "arch": "riscv64",
    "code-model": "medium",
    "cpu": "generic-rv64",
    "data-layout": "e-m:e-p:64:64-i64:64-i128:128-n64-S128",
    "eh-frame-header": false,
    "emit-debug-gdb-scripts": false,
    "executables": true,
    "features": "+m,+a,+f,+d,+c",
    "linker": "rust-lld",
    "linker-flavor": "ld.lld",
    "pre-link-args": {
        "ld.lld": [
            "--script=arch/riscv64/machines/qemu/kernel.ld",
            "-Map",
            "kernel.map"
        ]
    },
    "llvm-abiname": "lp64d",
    "llvm-target": "riscv64",
    "max-atomic-width": 64,
    "panic-strategy": "abort",
    "relocation-model": "static",
    "target-pointer-width": "64",
    "os": "none"
}
